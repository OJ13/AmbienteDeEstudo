<configuration>
  <system.web>
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />

    <authentication mode="Forms">
      <forms loginUrl="Login.aspx" name="sqlAuthCookie" timeout="60" path="/"></forms>
    </authentication>
    <authorization>
      <deny users="?"/>
      <allow users="*"/>
    </authorization>    
    
  </system.web>

  <connectionStrings>
    <add name="DevMediaSchoolConnectionString" connectionString="Data Source = N377; Initial Catalog = DevmediaSchool; Integrated Security = True"/>
  </connectionStrings>
  
</configuration>